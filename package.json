{
  "name": "ts-express-quickstart",
  "version": "1.0.0",
  "description": "A quickstart template for building Node.js applications using Express and TypeScript.",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alphajoop/ts-express-quickstart.git"
  },
  "keywords": [
    "ts",
    "node",
    "express"
  ],
  "author": "Alpha DIOP",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alphajoop/ts-express-quickstart/issues"
  },
  "homepage": "https://github.com/alphajoop/ts-express-quickstart#readme",
  "dependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.8",
    "express": "^4.19.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2"
  }
}
